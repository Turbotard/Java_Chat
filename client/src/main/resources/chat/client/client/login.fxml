<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="ap_login" fx:controller="chat.client.client.LoginController"
            prefHeight="200" prefWidth="300"
            xmlns:fx="http://javafx.com/fxml%22%3E">
    <children>
        <GridPane layoutX="50" layoutY="50" hgap="10" vgap="10">
            <Label text="Username" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
            <TextField fx:id="tf_username" GridPane.columnIndex="1" GridPane.rowIndex="0"/>
            <Button fx:id="button_login" text="Login" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
        </GridPane>
    </children>
</AnchorPane>